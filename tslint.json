{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "src/lib/**/*",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "ordered-imports": false,
    "member-access": false,
    "object-literal-sort-keys": false,
    "jsx-no-lambda": false,
    "no-unused-expression": false,
    "no-unused-variable": false
  }
}
