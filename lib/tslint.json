{
  "extends": ["tslint:recommended"],
  "linterOptions": {
    "exclude": [
      "node_modules/**/*.ts",
      "dist/**/*.ts"
    ]
  },
  "rules": {
    "quotemark": {
      "options": "single"
    },
    "member-access": false,
    "object-literal-sort-keys": false,
    "jsx-no-lambda": false
  }
}
